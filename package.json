{
  "name": "aline-knowledge-importer",
  "version": "1.0.0",
  "description": "Automatically ingest technical knowledge from blogs, guides, and PDFs into Aline knowledge base",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts"
  },
  "bin": {
    "scrape": "./dist/cli.js"
  },
  "keywords": [
    "knowledge-base",
    "web-scraping",
    "pdf-parsing",
    "firecrawl",
    "aline"
  ],
  "author": "Aline Team",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@mendable/firecrawl-js": "^1.29.0",
    "commander": "^12.0.0",
    "dotenv": "^16.0.0",
    "lowdb": "^7.0.0",
    "p-limit": "^5.0.0",
    "pdf-parse": "^1.1.1",
    "express": "^4.18.0",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "winston": "^3.11.0",
    "chalk": "^5.3.0",
    "ora": "^8.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/express": "^4.17.0",
    "@types/cors": "^2.8.0",
    "@types/pdf-parse": "^1.1.0",
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "prettier": "^3.2.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  }
} 